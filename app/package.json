    "express-validator": "^6.14.3",
    "helmet": "^8.1.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.30.1",
    "morgan": "~1.9.1",
    "mysql2": "^3.2.0",
